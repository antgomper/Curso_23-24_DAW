@ECHO OFF
SET ASTER=*
SET VECES=%1
SET VECES2=%VECES%

:inicio
IF %VECES2% EQU 0 GOTO fin

:inicio1
IF %VECES% EQU 1 GOTO fin1
SET ASTER=%ASTER% *
SET /A VECES=%VECES%-1
GOTO inicio1
:fin1

ECHO %ASTER%
SET ASTER=*
SET /A VECES=%VECES2%-1
SET /A VECES2=%VECES2%-1
GOTO inicio

:fin



